{
  "name": "react-boilerplate",
  "description": "RB-Lab's React boilerplate",
  "version": "0.0.1",
  "scripts": {
    "test": "karma start --single-run src/scripts/",
    "start": "node webpack/dev-server.js",
    "build": "if [ -d ./public ]; then rm -r ./public; fi && node webpack/build.js",
    "release": "if [ -d ./public ]; then rm -r ./public; fi && node webpack/build.js --production"
  },
  "license": "MIT",
  "dependencies": {
    "lodash": "^3.10.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  },
  "devDependencies": {
    "array.from": "^0.3.0",
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-rewire": "^1.0.0-beta-4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.18.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.15.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^1.6.1",
    "karma": "^0.13.19",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-sinon-chai": "^1.1.0",
    "karma-webpack": "^1.7.0",
    "minimist": "^1.2.0",
    "mocha": "^1.20.1",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfill": "0.0.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^2.0.1",
    "webpack": "^1.3.0-beta5",
    "webpack-dev-server": "^1.4.6"
  }
}
